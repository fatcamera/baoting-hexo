<% if(page.prev || page.next) { -%>
    <div class="pagination-container">
    <% if (page.prev) { -%>
        <div class="pagination"><a href="<%- url_for(page.prev_link) %>"><%- __('next')%></a></div>
    <% } -%>
    <% if (page.next) { -%>
        <div class="pagination"><a href="<%- url_for(page.next_link) %>"><%- __('prev')%></a></div>
    <% } -%>
    </div>
<% } -%>